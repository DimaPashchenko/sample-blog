<h1>Contact us</h1>

<%= form_with model: @contact, url: contacts_path, data:{turbo: false} do |f| %> 

	<p>
		<!-- Title<br /> -->
		<%= f.label :email %><br />
		<%= f.text_field :email %>
	</p>

	<p>
		<%= f.label :message %><br />
		<%= f.text_area :message %>
	</p>

	<p>
		<%= f.submit 'Send message' %>
	</p>
<% end %>